<div class="well">
    <h3>Edit Journal Entry</h3>
    <form action="/profile/edit/?_method=PUT" method="POST">
      <div class="form-group">
        <label for="title">Title</label>
        <input id="title" class="form-control" type="text" name="title" placeholder="<%=user.email%>">
      </div>
    
      <div class="form-group">
        <label for="date">Date</label>
        <input id="date" class="form-control" type="date" name="date">
      </div>
      
      <div class="form-group">
        <label for="content">Observation Notes</label>
        <input id="content" class="form-control" type="text" name="content" placeholder="<%= user.img %>">
      </div>
  
      <input class="btn btn-primary" type="submit">
    </form>
  </div>
  